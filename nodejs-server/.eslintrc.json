{
    "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "parser": "babel-eslint",
    "rules": {
        "prettier/prettier":"error",
        "no-unused-vars":"warn",
        "func-names":"off",
        "no-process-exit":"off",
        "object-shorthand":"off",
        "class-methods-use-this":"off"
        // "no-console":"off"
    }
}
